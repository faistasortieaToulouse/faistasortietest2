async function checkUrls() {
  const results: Record<string, any> = {
    timestamp: new Date().toISOString(),
    categories: [],
  };

  for (const category of categories) {
    const categoryResult: any = {
      title: category.title,
      links: [],
    };

    for (const url of category.links) {
      let status = '';
      try {
        const res = await fetch(url, { method: 'HEAD' });
        status = res.ok ? 'OK' : 'Lien cassé';
      } catch (e) {
        status = 'Lien cassé';
      }

      categoryResult.links.push({
        url,
        status,
        message:
          status === 'OK'
            ? ''
            : "Attention ! L'adresse de cette page a changée, merci de parcourir le site web.",
      });
    }

    results.categories.push(categoryResult);
  }

  fs.writeFileSync(
    'src/lib/checkUrlsResults.json',
    JSON.stringify(results, null, 2)
  );

  console.log('Vérification terminée, rapport mis à jour avec timestamp.');
}

checkUrls();
